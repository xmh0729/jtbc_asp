            <table cellpadding="0" cellspacing="0" width="100%">
              <tr>
                <td height="25" colspan="3"><img src="{$images}icon/topic.gif" border="0" /> <a href="./?type=detail&sid={$sid}&tid={$tid}">{$topic}</a></td>
