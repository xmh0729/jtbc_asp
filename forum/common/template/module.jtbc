      <tpl_default><![CDATA[    <table cellpadding="5" cellspacing="1" class="tables">
      <tr>
        <td class="tbt">{$=itake('module.img', 'lng')}</td>
      </tr>
      <tr>
        <td height="50" align="center"><img src="{$images}state/forum.gif" border="0" class="absmiddle" />&nbsp;{$=itake('module.forum', 'lng')}&nbsp;<img src="{$images}state/forum_new.gif" border="0" class="absmiddle" />&nbsp;{$=itake('module.forum_new', 'lng')}&nbsp;<img src="{$images}state/forum_lock.gif" border="0" class="absmiddle" />&nbsp;{$=itake('module.forum_lock', 'lng')}</td>
      </tr>
    </table>]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[data_manage_menu]]></name>
      <tpl_default><![CDATA[    <table cellpadding="5" cellspacing="1" class="tables">
      <tr>
        <td class="tbt">{$=itake('config.manage', 'lng')}</td>
