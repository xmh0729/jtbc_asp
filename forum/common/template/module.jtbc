              <tr>
                <td height="25" colspan="3"><img src="{$images}icon/time.gif" border="0" /> {$time}</td>
              </tr>
              <tr>
                <td width="33%" height="25"><img src="{$images}icon/num_new.gif" border="0" /> {$num_new}</td>
                <td width="33%"><img src="{$images}icon/num_topic.gif" border="0" /> {$num_topic}</td>
                <td width="33%"><img src="{$images}icon/num_note.gif" border="0" /> {$num_note}</td>
              </tr>
            </table>{$$}
            <table cellpadding="0" cellspacing="0" width="100%">
              <tr>
                <td align="center" class="ash">{$=itake('module.popedom', 'lng')}</td>
              </tr>
            </table>{$recurrence_ida}
]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[login]]></name>
      <tpl_default><![CDATA[{$recurrence_ida}
      <form action="{$=get_actual_route(userfolder)}/index.asp?action=login&backurl={$=urlencode(nurl)}" method="post">
      <table width="100%" cellpadding="0" cellspacing="0" border="0">
        <tr>
          <td height="25">{$=itake('global.lng_config.username','lng')} <input type="text" name="username" size="15" class="text" />&nbsp;{$=itake('global.lng_config.password','lng')} <input type="password" name="password" size="15" class="text" />{$recurrence_valcode} {$=itake('global.lng_config.valcode', 'lng')} <input type="text" name="valcode" size="4" class="text" /> {$=valcode}{$recurrence_valcode}&nbsp;<input type="submit" value="{$=itake('global.' & userfolder & ':api.login','lng')}" class="button"> <img src="{$global.images}public/small/sico.gif" border="0" /> <a href="{$=get_actual_route(userfolder)}">{$=itake('global.' & userfolder & ':api.register_newuser','lng')}</a> <img src="{$global.images}public/small/sico.gif" border="0" /> <a href="{$=get_actual_route(userfolder)}/?type=lostpassword">{$=itake('global.' & userfolder & ':api.lost_password", "lng")}</a></td>
        </tr>
      </table>
      </form>{$$}
      <table border="0" cellpadding="0" cellspacing="0" width="100%">
        <tr>
          <td height="25">{$=nusername}, <a href="{$=get_actual_route(userfolder)}/message/?type=list" target="_self">{$message}</a>, <a href="{$=get_actual_route(userfolder)}/?type=manage">{$=itake('global.' & userfolder & ':api.membercenter','lng')}</a>, <a href="{$=get_actual_route(userfolder)}/?action=logout&backurl={$=urlencode(nurl)}" onclick="return click_return('{$=itake('global.' & userfolder & ':api.logoutstr', 'lng')}')">{$=itake('global.' & userfolder & ':api.logout','lng')}</a></td>
        </tr>
      </table>{$recurrence_ida}]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[index]]></name>
      <tpl_default><![CDATA[{$=web_head(nhead)}
<div id="middle">
  <div id="middleContent2">{$=module_data_top()}{$recurrence_forum}
    <table cellpadding="5" cellspacing="1" class="tables">
      <tr>
        <td colspan="3" class="tbt">{$sort}</td>
      </tr>{$recurrence_ida}
      <tr>
        <td width="60" align="center"><img src="{$images}state/{$pic}.gif" border="0" /></td>
        <td>
          <table width="100%" cellpadding="0" cellspacing="0">
