          <input type="hidden" name="pgenre" value="{$pgenre}" />
          <table width="100%" cellpadding="0" cellspacing="0" border="0">
            <tr>
              <td height="25">{$=itake('module.orderid','lng')}</td>
            </tr>
            <tr>
              <td height="25"><input type="text" name="orderid" size="15" maxlength="18"></td>
            </tr>
            <tr>
              <td height="25"><input type="submit" value="{$=itake('global.lng_config.submit','lng')}" class="button"></td>
            </tr>
          </table>
          </form>
        </div>
      </div>
    </div>
    <div class="middleMain">
      <div class="middleMainContent">
        <table cellpadding="0" cellspacing="0" class="tablen">
          <tr>
            <td class="nav"><span>{$=inavigation("module:", 0)}</span></td>
          </tr>
          <tr>
            <td class="list" valign="top">
              <table width="100%" cellspacing="0" cellpadding="0" border="0">
                <tr>
                  <td height="25"><strong>{$=itake('module.orderinfo', 'lng')}</strong></td>
                </tr>
                <tr>
                  <td height="25">{$=itake('module.orderid', 'lng')}{$orderid}</td>
                </tr>
                <tr>
                  <td height="10"></td>
                </tr>
                <tr>
                  <td height="25" class="red">{$paystr}</td>
                </tr>
                <tr>
                  <td height="10"></td>
                </tr>
              </table>
            </td>
          </tr>
          <tr>
            <td height="10"></td>
          </tr>
        </table>
      </div>
    </div>
    <div class="clear"></div>
  </div>
</div>
{$=web_foot(nfoot)}
]]></tpl_default>
    </item>
  </item_list>
</xml>
