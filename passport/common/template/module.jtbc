    <item>
      <name><![CDATA[register]]></name>
      <tpl_default><![CDATA[{$=web_head(nhead)}
{$=echo_error()}
<script type="text/javascript" src="{$global.images}js/validator.js"></script>
<script type="text/javascript">
function jscheck_usernames(strers)
{
  var tstrers = strers;
  get_id("trck_username").style.display = "";
  var check_username_str_0 = "{$=itake('validator.check_username_0', 'lng')}"
  var check_username_str_1 = "{$=itake('validator.check_username_1', 'lng')}"
  var check_username_str_error = "{$=itake('validator.check_username_error', 'lng')}"
  var check_username_str = "{$=itake('validator.check_username', 'lng')}";
  if (tstrers == "1")
  {
    get_id("ck_username").innerHTML = check_username_str_1;
  }
  else
  {
    if (tstrers == "0")
    {
      get_id("ck_username").innerHTML = check_username_str_0;
    }
    else
    {
      get_id("ck_username").innerHTML = check_username_str_error;
    };
  };
};

function jscheck_username(struname)
{
  if (struname == "") return false;
  var check_username_ing = "{$=itake('validator.check_username_ing', 'lng')}";
  get_id("ck_username").innerHTML = check_username_ing;
  igets("interface.asp?type=check_username&username=" + struname, jscheck_usernames);
};
</script>
<div id="middle">
  <div id="middleContent">
    <div class="middleSide">
      <div class="box1">
