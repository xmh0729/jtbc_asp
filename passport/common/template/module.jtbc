            </tr>
            <tr>
              <td height="25" align="left">{$=itake('module.process_a','lng')}</td>
            </tr>
            <tr>
              <td height="25" align="left">{$=itake('module.process_b','lng')}</td>
            </tr>
            <tr>
              <td height="25" align="left">{$=itake('module.process_c','lng')}</td>
            </tr>
            <tr>
              <td height="25" align="left">{$=itake('module.process_d','lng')}</td>
