                    </tr>{$recurrence_idb}
                  </table>
                  <table border="0" width="98%" cellSpacing="0" cellPadding="0">
                    <tr>
                      <td width="100%" height="25" align="right">{$cpagestr}</td>
                    </tr>
                    <tr>
                      <td width="100%" height="5"></td>
                    </tr>
                  </table>
                  <table border="0" width="98%" cellSpacing="0" cellPadding="0">
                    <tr>
                      <td width="5" height="25"></td>
                      <td width="30"><a href="javascript:switch_display('batch_control');"><img src="{$global.images}public/small/zico.gif" border="0" alt="{$=itake('global.lng_admin.batch_control','lng')}"></a></td>
                      <td width="*" align="right">{$=sel_control}</td>
                    </tr>
                  </table>
                  <table border="0" width="98%" cellSpacing="0" cellPadding="0">
                    <tr>
                      <td width="100%" height="5"></td>
                    </tr>
                  </form>
                  </table>
                </td>
              </tr>
            </table>
            <table border="0" width="100%" cellSpacing="0" cellPadding="0">
              <tr>
                <td width="100%" height="5"></td>
              </tr>
            </table>
            <table border="0" width="100%" cellSpacing="0" cellPadding="0" class="lrbtline" id="batch_control" style="DISPLAY: none">
              <tr>
                <td width="100%" height="25" class="tbtop">{$=itake('global.lng_admin.batch_control','lng')}</td>
              </tr>
              <tr>
                <td width="100%" align="center">
                  <table border="0" width="98%" cellSpacing="0" cellPadding="0">
                  <form method="post" action="?action=batch_control&backurl={$=urlencode(nurl)}&{$=nurs}">
                    <tr>
                      <td width="100%" height="5"></td>
                    </tr>
                    <tr>
                      <td width="100%" height="25" align="left">{$=itake('global.lng_admin.batch_control_s0','lng')}&nbsp;<select name="bth_type" class="select" onchange="if (this.value == 'delete') get_id('span_group').style.display = 'none'; else get_id('span_group').style.display = '';">{$=show_xmlinfo_select("global.sel_control.all|" & ncontrol, "", "select")}</select>&nbsp;&nbsp;<span id="span_group">{$=itake('global.lng_admin.batch_control_s1_2','lng')}&nbsp;<select name="bth_group" class="select"><option value="">{$=itake('global.lng_admin.batch_control_s2','lng')}</option>{$=show_xmlinfo_select("sel_group.all","","select")}</select>&nbsp;</span><input type="submit" name="submit" value="{$=itake('global.lng_config.control','lng')}" class="button" onclick="return click_return('{$=itake('global.lng_admin.batch_control_sure','lng')}')"></td>
                    </tr>
                    <tr>
                      <td width="100%" height="5"></td>
                    </tr>
                  </form>
                  </table>
                </td>
              </tr>
            </table>
          </td>
        </tr>
      </table>
    </td>
  </tr>
  <tr>
    <td width="100%" height="5"></td>
  </tr>
</table>
]]></tpl_default>
    </item>
    <item>
