                <td width="100%" height="25" class="tbtop">{$=itake('manage.manage','lng')}</td>
              </tr>
              <tr>
                <td width="100%" align="center">
                  <table border="0" width="98%" cellSpacing="0" cellPadding="0">
                    <tr>
                      <td width="100%" height="5"></td>
                    </tr>
                  </table>
                  <table border="0" width="98%" cellSpacing="0" cellPadding="0">
                    <form method="post" name="sel_form" action="?action=control&backurl={$=urlencode(nurl)}">
                    <tr>
                      <td height="25" width="30"><input type="checkbox" name="sel_all" class="checkbox" onClick="select_all()"></td>
                      <td width="*">{$=itake('config.username','lng')}</td>
                      <td width="150">{$=itake('config.email','lng')}</td>
                      <td width="50">{$=itake('config.sex','lng')}</td>
                      <td width="50">{$=itake('config.old','lng')}</td>
                      <td width="120">{$=itake('config.time','lng')}</td>
                      <td width="80">{$=itake('config.group','lng')}</td>
                      <td align="center" width="50">{$=itake('global.lng_config.edit','lng')}</td>
                      <td align="center" width="50">{$=itake('global.lng_config.delete','lng')}</td>
                    </tr>{$recurrence_idb}
                    <tr>
                      <td height="25"><input type="checkbox" name="sel_id" value="{$id}" class="checkbox"></td>
                      <td><a href="?type=edit&backurl={$=urlencode(nurl)}&id={$id}">{$username}</a></td>
                      <td>{$email}</td>
                      <td>{$sex}</td>
                      <td>{$old}</td>
                      <td>{$time}</td>
                      <td>{$group}</td>
